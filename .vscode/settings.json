{
    "go-template.patterns": [
        "**/*.gotmpl"
    ],
    "go-template.languages": [
        "go"
    ],
    "yaml.schemas": {
        "./.vscode/enum.schema.json": [
            "**/*.enum.yaml"
        ]
    },
    "json.schemas": [
        {
            "fileMatch": ["**/*.enum.json"],
            "url": "./.vscode/enum.schema.json"
        }
    ],
    "cssvar.files": [
        "./docs/node_modules/open-props/open-props.min.css",
        // if you have an alternative path to where your styles are located
        // you can add it in this array of files
        "assets/styles/variables.css"
      ],
    
      // Do not ignore node_modules css files, which is ignored by default
      "cssvar.ignore": [],
    
      // add support for autocomplete in JS or JS like files
      "cssvar.extensions": [
        "css", "postcss", "jsx", "tsx", "svelte"
      ],
      "deno.enable": true,
        "deno.lint": true,
  "editor.formatOnSave": true,
  "[typescript]": { "editor.defaultFormatter": "denoland.vscode-deno" }
}
