{
    "go-template.patterns": [
        "**/*.gotmpl"
    ],
    "go-template.languages": [
        "go"
    ],
    "yaml.schemas": {
        "./.vscode/enum.schema.json": [
            "**/*.enum.yaml"
        ]
    },
    "json.schemas": [
        {
            "fileMatch": [
                "**/*.enum.json"
            ],
            "url": "./.vscode/enum.schema.json"
        }
    ],
    "editor.formatOnSave": true,
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    }
}